@model ASP.NET_MVC_MajsterStrelby.Models.PlayersViewModel
@{
    ViewBag.Title = "Leaderboard";
}
<div class="jumbotron">
    <h2>Rebríček</h2>
    <ul>
        @foreach (var player in Model.GetPlayers())
        {
            <li>
                @player._name , @player._level , @player._resolved
            </li>
        }
        <p></p>
        <li>@Model.GetActualPlayer()._name , @Model.GetActualPlayer()._level, @Model.GetActualPlayer()._resolved</li>
    </ul>
       

</div>
