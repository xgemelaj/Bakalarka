@model ASP.NET_MVC_MajsterStrelby.ViewModels.PlayersViewModel
@{
    ViewBag.Title = "Leaderboard";
}
<div class="jumbotron">
    <h2>Leaderboard</h2>
    <ul>
        @foreach (var player in Model._players)
        {
            <li>
                @player._name , @player._points , @player._resolved
            </li>
        }
    </ul>
</div>
